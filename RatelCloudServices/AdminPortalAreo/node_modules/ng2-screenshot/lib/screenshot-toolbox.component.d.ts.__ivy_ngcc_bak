import { ElementRef, AfterViewInit } from '@angular/core';
import { ScreenshotComponent } from './screenshot.component';
/**
 * Custom template of screenshot toolbox, connent with screenshot
 */
export declare class ScreenshotToolboxComponent implements AfterViewInit {
    parent: ScreenshotComponent;
    wrapper: ElementRef;
    hasContent: boolean;
    constructor(parent: ScreenshotComponent);
    cancel(): void;
    download(): void;
    ngAfterViewInit(): void;
}
